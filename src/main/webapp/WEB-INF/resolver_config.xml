        <resolver>
            <name>eDISS: Dissertationen der Georg-August-Universität Göttingen</name>
            <url>http://ediss.uni-goettingen.de/resolvexml?</url>           
        </resolver> 
	<resolver>
            <name>Dspace Goedoc</name>
            <url>http://goedoc.tc.sub.uni-goettingen.de/resolvexml/</url>
	</resolver>
    </localresolver>
